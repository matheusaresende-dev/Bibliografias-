@article{Jiang2026,
   abstract = {Emulsion gels based on biomacromolecules have emerged as promising alternatives to those stabilized by traditional surfactants, addressing safety concerns in food applications. High amylose maize starch (HAMS) exhibits exceptional gel-forming capability due to its unique amylose-to-amylopectin ratio, rendering it a promising material for emulsion gels. However, its strong hydrophilicity and dense granular structure restrict interfacial adsorption, limiting practical applications. Herein, an innovative emulsion gel system based solely on HAMS was developed by integrating octenyl succinic anhydride (OSA) modification with microwave treatment-assisted gelatinization, enabling efficient energy utilization, green clean-label production, and simplified processing. The effects of starch concentration (c = 2–8 %) and oil fraction (Φ = 30–60 %) on gel properties were systematically investigated to reveal the formation mechanism. Increasing the starch concentration or oil fraction improved droplet uniformity, self-supporting ability, gel strength, and water holding capacity. Furthermore, Cryo-SEM analysis revealed sufficient starch addition played a more critical role in stable gel network formation, which formed ''sheet-like'' networks, whereas oil droplets acted as active fillers and reinforced the gel matrix through interfacial starch-starch interactions. The stability analyzer indicated that higher starch concentrations and oil fractions synergistically enhanced the storage stability of emulsion gels. This study provides a green, clean-label strategy for HAMS utilization in emulsion gels, alongside insights into structure-function relationships and potential applications in fat replacement and bioactive delivery.},
   author = {Chengchen Jiang and Danli Jin and Ziqi Chen and Zhengbiao Gu and Li Cheng and Zheming Zhang and Yan Hong},
   doi = {https://doi.org/10.1016/j.foodhyd.2025.111918},
   issn = {0268-005X},
   journal = {Food Hydrocolloids},
   keywords = {Emulsion gel,Gel properties,High amylose maize starch,Microwave treatment,Octenyl succinic anhydride},
   pages = {111918},
   title = {Emulsion gels based on OSA modified high amylose maize starch: Formation, interfacial properties, and stability},
   volume = {172},
   url = {https://www.sciencedirect.com/science/article/pii/S0268005X25008781},
   year = {2026}
}
@article{Shi2026,
   abstract = {With the rapid development of information technology including artificial intelligence, the issue related to power consumption of current electrical and electronic devices has become increasingly serious. Hence, there is a pressing need to design and develop high-performance materials that can meet the critical demands for low power consumption and high energy conversion efficiency. Soft magnetic amorphous alloys (SMAAs) and their derivatives, mainly including soft magnetic nanocrystalline alloys (SMNAs), are nowadays state-of-the-art energy-saving materials due to their high permeability (μ), low coercivity (Hc), low saturation magnetostriction (λs) and high saturation magnetic induction (Bs), which result in low core loss and high energy conversion efficiency, particularly for high-frequency applications. Over the past few decades, compositional design, structural modification and subsequent process control have been utilized to enhance Bs, increase μ, reduce Hc and decrease λs. Through a comprehensive survey on these results in literature, this review article aims to clarify the key factors influencing the soft magnetic properties of SMAAs/SMNAs from both experimental and theoretical viewpoints and further uncover the mechanisms underlying the correlations among composition, structure, processing and properties as well as their coupling effects. In addition, the current industrial application status of SMAAs/SMNAs is summarized together with the related technological challenges that impede their potential applications. To sustain the rapid development of SMAAs/SMNAs, new perspectives are also proposed for making possible breakthroughs in their soft magnetic properties and cost performance, which may trigger new research realm and further extend their application range.},
   author = {Lingxiang Shi and Tiantian Chai and Xiangning Du and Jili Jia and Kefu Yao and Zhengjun Zhang and Na Chen},
   doi = {https://doi.org/10.1016/j.mser.2025.101078},
   issn = {0927-796X},
   journal = {Materials Science and Engineering: R: Reports},
   keywords = {Fe-based amorphous alloys,Soft magnetic amorphous alloys,Soft magnetic nanocrystalline alloys,Soft magnetic properties},
   pages = {101078},
   title = {Soft magnetic amorphous alloys and their derivatives},
   volume = {167},
   url = {https://www.sciencedirect.com/science/article/pii/S0927796X25001561},
   year = {2026}
}
@article{Grilo2026,
   abstract = {Electric power systems are continuously evolving to meet the growing demand. International agreements aim to increase the share of sustainable energy sources, which enhances the relevance of inverter-based resources (IBRs) interconnection lines. However, in power grids with IBRs, protection systems face challenges due to the behavior of voltages and currents. While phasor-based fault location is widely used, it is significantly impacted by fault resistance. Thus, the present article proposes an approach that combines apparent impedance techniques with machine learning (ML), using data exclusively from the substation, to accurately estimate fault locations in IBRs interconnection lines. Initially, a ML-based approach estimates the fault resistance and applies a compensation factor to estimate the fault current. The approach proved promising, reducing average errors in the fault location task to less than 1% without requiring new meters in the system, communication structures between transmission line terminals, or meters with higher sampling frequencies.},
   author = {C V C Grilo and M.J.B.B. Davi and L S Lessa and D V Coury and R A S Fernandes},
   doi = {https://doi.org/10.1016/j.epsr.2025.112140},
   issn = {0378-7796},
   journal = {Electric Power Systems Research},
   keywords = {Fault location,Impedance-based fault location,Inverter-based resources,Machine learning},
   pages = {112140},
   title = {Improving fault location in wind farm interconnection lines through fault resistance estimation},
   volume = {250},
