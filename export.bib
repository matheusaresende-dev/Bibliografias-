@inbook{Urion2026,
   abstract = {This chapter considers two interrelated issues: how should child neurologists ideally be educated and trained in terms of their early development and introduction to the field, and how do these issues of education and training have an influence on the workforce in the first half of the current century. The consideration of the issues of education and training, as well as the workforce in child neurology, directly affects the very nature and identity of child neurology. How we educate and train those who will follow us, to whom we are accountable for the content and outcome of that education, and how we deploy the workforce we thus prepare define our very essence as child neurologists. These issues have been substantially altered by the effects on training and the workforce of the COVID-19 pandemic, through which we are still passing at the time of this writing. The effects of that event have been both so widespread and so subtle that it will likely take years for a complete understanding of how this changed medicine in general and child neurology in particular to emerge.},
   author = {David K Urion and Phillip L. Pearl and Stephen Ashwal},
   city = {New York},
   doi = {https://doi.org/10.1016/B978-0-443-10944-7.00019-6},
