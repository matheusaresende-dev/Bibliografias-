   keywords = {Bi/BiMoO,P-doped ultrathin porous g-CN,Photoelectrochemical aptasensor,Tetracycline detection,Z-Scheme heterojunction},
   pages = {128784},
   title = {A sensitive photoelectrochemical aptasensor based on Bi/Bi2MoO6-modified phosphorus-doped ultrathin g-C3N4 formed Z-scheme heterojunction for tetracycline detection},
   volume = {297},
   url = {https://www.sciencedirect.com/science/article/pii/S0039914025012755},
   year = {2026}
}
@article{Standaert2026,
   abstract = {Process innovation is a crucial driver of firms’ competitiveness, but difficulties in knowledge transfer make it challenging. Drawing on the ability-motivation-opportunity framework for knowledge transfer, we propose that the scale on which firms deploy AI technologies has a positive impact on the likelihood they introduce process innovations, as AI overcomes human limitations related to the ability and motivation for knowledge transfer. Moreover, we argue that this relationship will be more pronounced when there is lower opportunity for interpersonal knowledge transfer, and in particular when firms (a) have a large number of employees, (b) do not provide on-site employee training, and (c) have higher employee turnover. We use a unique combination of survey and social balance sheet data on a sample of 2268 Belgian firms. Heckman maximum-likelihood probit models and several robustness tests confirm the majority of our hypotheses. The study enriches the literature on process innovation and knowledge management, and provides important theoretical and practical insights on how and under which circumstances AI can lead to a competitive advantage.},
   author = {Thomas Standaert and Petra Andries},
   doi = {https://doi.org/10.1016/j.technovation.2025.103350},
   issn = {0166-4972},
   journal = {Technovation},
   keywords = {Artificial intelligence,Interpersonal interaction,Knowledge,Knowledge transfer,Process innovation},
   pages = {103350},
   title = {Overcoming difficulties in knowledge transfer: Harnessing the power of AI to drive process innovation},
   volume = {149},
   url = {https://www.sciencedirect.com/science/article/pii/S0166497225001828},
   year = {2026}
}
@inbook{Kuziemsky2026,
   abstract = {This chapter describes the digital health management journey from aspiration and planning to implementation and evaluation and achieving sustainable operations and scale-up. This sociotechnical enterprise-wide endeavor incorporates the leadership commitment, strategic and financial planning, strategic resource allocation, change management, procurement, and technology adoption and implementation that evolves over time. Managing the envisioning and planning phase involves assessing DHM as a strategic requirement for establishing realistic goals by (1) determining current maturity, (2) identifying the gaps and opportunities, and, accordingly, (3) prioritizing investment, funding, and resource allocations to facilitate optimal conditions for the digital health program to thrive. During the implementation and operations phase, DHM remains a guiding principle for (1) managing the overall performance of a digital health program in alignment with the organization's strategic priorities, (2) change management processes, and (3) evaluating the impacts of implemented changes to the organization's digital health maturity. The DHM can guide a systematic approach to uncover new ways to scale-up program impact for the end users and health ecosystem partners.},
   author = {Craig Kuziemsky and Rui Zhou and Siaw-Teng Liaw},
