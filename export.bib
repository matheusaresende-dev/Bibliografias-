   journal = {Cities},
   keywords = {Competitive advantage,Drivers,KM,Knowledge capture,Knowledge management,Knowledge sharing,Smart cities},
   pages = {106411},
   title = {Assessing drivers for managing knowledge in the context of smart cities},
   volume = {168},
   url = {https://www.sciencedirect.com/science/article/pii/S0264275125007127},
   year = {2026}
}
@article{Do2026,
   abstract = {Oxygen utilization in proton exchange membrane fuel cells (PEMFCs) plays a critical role, particularly in unmanned underwater vehicle (UUV) applications, where oxygen availability is limited. To enhance oxygen utilization, the dead-end cathode mode (DEC) is employed and investigated under various purge operations at operating load levels in this study. Oxygen utilization is experimentally determined and used to train an artificial neural network (ANN) model via the Levenberg–Marquardt algorithm at purge durations of 100, 500, and 900 ms. The ANN model is applied to predict oxygen utilization at purge durations of 300 and 700 ms, which are validated against experimental data. Furthermore, oxygen utilization is predicted at new purge durations of 200, 400, 600, and 800 ms. The model performance is evaluated and demonstrates a high correlation coefficient (R2), low mean absolute error (MAE), root mean square error (RMSE), and mean absolute percentage error (MAPE). The results indicate that, at the optimal purge duration of 100 ms and purge intervals of 35, 110, and 300 s, the maximum oxygen utilization of 9.4, 11.2, and 13.0 kJ L−1 is achieved under varying load levels of 0.8, 0.4, and 0.1 A cm−2, respectively. A cathode purge strategy for PEMFC is developed to optimize oxygen utilization for UUVs.},
   author = {Tan-Thich Do and Vinh-Dat Ly and Amornchai Arpornwichanop and Yong-Song Chen},
   doi = {https://doi.org/10.1016/j.fuel.2025.136628},
   issn = {0016-2361},
   journal = {Fuel},
   keywords = {Artificial Neural Networks,Dead-end cathode,Prediction of oxygen utilization,Proton exchange membrane fuel cell,Purge strategy,UUVs},
   pages = {136628},
   title = {Prediction of oxygen utilization and purge strategy optimization for proton exchange membrane fuel cells using an artificial neural network approach},
   volume = {405},
   url = {https://www.sciencedirect.com/science/article/pii/S0016236125023531},
   year = {2026}
}
@inbook{Liaw2026,
   abstract = {This chapter describes the digital health maturity metamodel (DHM3), its operationalization as the Digital Health Profile & Maturity Assessment Toolkit (DHPMAT), and the co-creation methodology to ensure relevance, ownership, and social capital in the local community. The metamodel approach unifies digital health and integrates existing domain-specific maturity models. The five foundations (ICT and IoT infrastructure, Digital tools, Health information sharing, Enablers of trust, and Quality Improvement) are assessed at five maturity stages based on system performance and information sharing extent and quality. DHM3 asserts that all interventions, policies, and programs must advance the digital health maturity (DHM) and Quintuple Aim simultaneously. The DHPMAT systematically assesses DHM across the micro (individual), meso (organization), macro (national health system), and meta (global) levels of the enterprise. DHM stages are grounded on Stage 3 (Standardized), where the deliverable is an enterprise architecture and platform compliant with interoperability standards and quality benchmarks for data, tools, systems, and services. DHM stages are stable and managed, but the processes are dynamic, evolving nonlinearly due to intrinsic (e.g., digital assets, creators, and users) and extrinsic factors (e.g., geopolitics, priorities, and opportunities).},
