   keywords = {FJSP,Imperialist competitive algorithm,Limited auxiliary resources,Limited transportation resources,Reinforcement learning},
   pages = {129467},
   title = {Reinforcement learning enhanced imperialist competitive algorithm for flexible job-shop scheduling problem with limited transportation and auxiliary resources},
   volume = {297},
   url = {https://www.sciencedirect.com/science/article/pii/S0957417425030830},
   year = {2026}
}
